{
  "printWidth": 120,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "auto",
  "insertFinalNewline": true,
  "bracketSpacing": true,
  "arrowParens": "always",

  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindAttributes": ["classes"]
}
